package Entity;

import java.util.List;

import com.HospitalManagementSystem.Doctor.Doctor;



public class DoctInfo {
	
	Doctor doctor;
	List<PatientInfo> patient;

	public Doctor getDoctor() {
		return doctor;
	}

	public void setDoctor(Doctor doctor) {
		this.doctor = doctor;
	}

	public List<PatientInfo> getPatient() {
		return patient;
	}

	public void setPatient(List<PatientInfo> patient) {
		this.patient = patient;
	}
	
	
	
}
